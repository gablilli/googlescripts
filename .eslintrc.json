{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "globals": {
    "Logger": "readonly",
    "DriveApp": "readonly",
    "Classroom": "readonly",
    "ScriptApp": "readonly",
    "Utilities": "readonly",
    "UrlFetchApp": "readonly",
    "Drive": "readonly",
    "SpreadsheetApp": "readonly",
    "GmailApp": "readonly",
    "Session": "readonly",
    "CalendarApp": "readonly",
    "FormApp": "readonly",
    "MailApp": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error"
  }
}
